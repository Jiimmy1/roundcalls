<script>
    // Advanced counter system
    function animateCounter(counterElement, start, end, duration) {
        let startTime = null;
        
        const animation = (currentTime) =&gt; {
            if (!startTime) startTime = currentTime;
            const progress = currentTime - startTime;
            const increment = Math.floor(progress / duration * (end - start));
            
            if (increment &lt; end - start) {
                counterElement.textContent = start + increment;
                requestAnimationFrame(animation);
            } else {
                counterElement.textContent = end;
            }
        };
        
        requestAnimationFrame(animation);
    }

    // Trigger counters on scroll
    const counters = document.querySelectorAll('.counter-number');
    
    const observer = new IntersectionObserver((entries) =&gt; {
        entries.forEach(entry =&gt; {
            if (entry.isIntersecting) {
                const target = parseInt(entry.target.dataset.target);
                animateCounter(entry.target, 0, target, 2000);
            }
        });
    }, { threshold: 0.5 });

    counters.forEach(counter =&gt; observer.observe(counter));

    // Rotating text (from previous code)
    const rotateElement = document.querySelector('[data-rotate]');
    if (rotateElement) {
        const texts = rotateElement.dataset.rotate.split(',');
        let current = 0;
        
        function rotateText() {
            rotateElement.textContent = texts[current];
            current = (current + 1) % texts.length;
            setTimeout(rotateText, 3000);
        }
        rotateText();
    }
</script>
</template>
