<script>
    // Enhanced Counter System
    const animateCounters = () =&gt; {
        const counters = document.querySelectorAll('.stat-number');
        
        counters.forEach(counter =&gt; {
            const target = parseInt(counter.dataset.target);
            let count = 0;
            
            const updateCounter = () =&gt; {
                if (count &lt; target) {
                    count++;
                    counter.textContent = count;
                    setTimeout(updateCounter, 50);
                }
            }
            
            updateCounter();
        });
    }

    // Optimized Parallax Scroll Effect
    window.addEventListener('scroll', () =&gt; {
        const scrolled = window.pageYOffset;
        if(document.querySelector('.hero')) {
            document.querySelector('.hero').style.transform = `translateY(${scrolled * 0.5}px)`;
        }
        if(document.querySelector('.card-grid')) {
            document.querySelector('.card-grid').style.transform = `translateY(${scrolled * 0.3}px)`;
        }
    });

    // Initialize Counters on Load
    window.addEventListener('load', animateCounters);

    // Image Hover Effects
    document.querySelectorAll('.client-photo').forEach(photo =&gt; {
        photo.addEventListener('mouseenter', () =&gt; {
            photo.style.transform = 'rotate(5deg) scale(1.1)';
        });
        
        photo.addEventListener('mouseleave', () =&gt; {
            photo.style.transform = 'none';
        });
    });
</script></template>
