        overflow: hidden;
        margin-bottom: 2rem;
    }

    .service-card:hover {
        transform: translateY(-10px) scale(1.05);
        background: rgba(255,255,255,0.15);
        box-shadow: 0 25px 50px rgba(0,0,0,0.3);
    }

