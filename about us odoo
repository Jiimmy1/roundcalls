<script>
    // Scroll reveal effects
    const observer = new IntersectionObserver((entries) =&gt; {
        entries.forEach(entry =&gt; {
            if (entry.isIntersecting) {
                entry.target.style.opacity = 1;
                entry.target.style.transform = 'translateY(0)';
            }
        });
    });

    document.querySelectorAll('.card, .step, .cta-section').forEach(el =&gt; {
        el.style.opacity = 0;
        el.style.transform = 'translateY(20px)';
        observer.observe(el);
    });

    // Button hover interactivity
    document.querySelectorAll('.button').forEach(button =&gt; {
        button.addEventListener('mouseover', () =&gt; {
            button.style.background = 'linear-gradient(45deg, #2c3e50, #3498db)';
        });
        
        button.addEventListener('mouseout', () =&gt; {
            button.style.background = 'linear-gradient(45deg, #3498db, #2c3e50)';
        });
    });
</script>

