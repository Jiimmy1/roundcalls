    }

    .container {
        max-width: 1400px;
        margin: 0 auto;
        padding: 0 20px;
    }

    /* Pricing Section */
    .pricing-section {
        padding: 4rem 0;
        background: linear-gradient(135deg, #f0f4f8 0%, #ffffff 100%);
    }

    .pricing-card {
        background: white;
        border-radius: 25px;
        padding: 2.5rem;
        margin: 2rem 0;
        box-shadow: 0 15px 40px rgba(0,0,0,0.1);
        transition: transform 0.3s ease;
        position: relative;
        overflow: hidden;
    }

    .pricing-card:hover {
        transform: translateY(-10px);
    }

    .plan-comparison {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(300px, 1fr));
        gap: 2rem;
        margin-top: 3rem;
    }

    .plan-tier {
        padding: 2rem;
        border-radius: 20px;
        border: 2px solid var(--primary);
    }

    .feature-list {
        list-style: none;
        padding: 0;
    }

    .feature-item {
        display: flex;
        align-items: center;
        gap: 1rem;
        padding: 1rem;
        margin: 0.5rem 0;
        background: rgba(42,92,130,0.05);
        border-radius: 10px;
    }

    /* Interactive Comparison Table */
    .comparison-table {
        overflow-x: auto;
        margin: 3rem 0;
        border-radius: 20px;
        box-shadow: 0 10px 30px rgba(0,0,0,0.1);
    }

    table {
        width: 100%;
        border-collapse: collapse;
        background: white;
        min-width: 800px;
    }

    th, td {
        padding: 1.5rem;
        text-align: center;
        border-bottom: 2px solid #f0f0f0;
    }

    thead th {
        background: var(--primary);
        color: white;
        font-size: 1.1rem;
        position: sticky;
        left: 0;
    }

    .pro-badge {
        background: var(--accent);
        color: white;
        padding: 0.3rem 1rem;
        border-radius: 20px;
        font-size: 0.9rem;
    }

    /* Interactive Effects */
    .hover-effect {
        transition: all 0.3s ease;
        cursor: pointer;
    }

    .hover-effect:hover {
        background: rgba(50,180,193,0.05);
        transform: scale(1.02);
    }

    /* Buttons */
    .cta-button {
        background: var(--accent);
        color: white;
        padding: 1rem 2.5rem;
        border-radius: 30px;
        display: inline-flex;
        align-items: center;
        gap: 0.5rem;
        text-decoration: none;
        transition: all 0.3s ease;
    }

    .cta-button:hover {
        transform: translateY(-3px);
        box-shadow: 0 5px 15px rgba(255,107,107,0.3);
    }
</style>

