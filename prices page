  <script>
    function calculateCost() {
      // New pricing values
      const baseCalls = 1000; // Base number of calls
      const basePrice = 199; // Price for the first 1000 calls
      const callCost = 0.15; // Additional cost per call
      const employeeCost = 20; // Cost per employee
      const baseLanguages = 1; // Base languages
      const languageHourlyRate = parseFloat(document.getElementById('hourlyRate').value) || 15; // Hourly rate for language

      // Get input values
      const calls = parseInt(document.getElementById('calls').value) || 0;
      const employees = parseInt(document.getElementById('employees').value) || 1;
      const languages = parseInt(document.getElementById('languages').value) || 1;

      // Calculate call cost
      let callTotal = basePrice;
      if(calls &gt; baseCalls) {
        callTotal += (calls - baseCalls) * callCost;
      }

      // Calculate employee cost
      const employeeTotal = employees * employeeCost;

      // Calculate language cost (hourly rate for each language)
      const languageTotal = languages &gt; baseLanguages 
        ? (languages - baseLanguages) * languageHourlyRate * 30 // Assuming 30 hours/month per language
        : 0;

      // Calculate total cost
      const total = callTotal + employeeTotal + languageTotal;

      // Display result
      const resultElement = document.getElementById('result');
      if(calls &gt;= 0 &amp;&amp; employees &gt;= 1 &amp;&amp; languages &gt;= 1) {
        resultElement.innerHTML = `
          Estimated cost: $${total.toFixed(2)} per month 
          &lt;br&gt;
          &lt;small style="color: #666; font-size: 14px;"&gt;
            (${calls} calls + ${employees} employees + ${languages} languages)
          &lt;/small&gt;
        `;
        resultElement.style.color = "#002366";
      } else {
        resultElement.innerHTML = "Please enter valid values (calls ≥ 0, employees ≥ 1)";
        resultElement.style.color = "red";
      }
    }
  </script>
</div></div>
