    <div class="step-card" data-step="3">
        <div class="result-card">
            <h2>✅ Your Recommended Plan:</h2>
            <div id="recommendedPlan" style="font-size: 1.8rem; margin: 1rem 0"/>
            <div id="planPrice" style="font-size: 1.4rem"/>
            <div id="planFeatures" style="text-align: right; margin-top: 1.5rem"/>
        </div>
    </div>
</div>

<script>
document.addEventListener('DOMContentLoaded', function() {
    const plans = {
        basic: {
            name: "The Seed 🌳",
            price: 199,
            features: ["3 core languages (Arabic, English, German)", "500 minutes of calls/month", "Basic chatbot", "Weekly performance reports", "12 hours/day technical support"],
            suitableFor: ["individual", "small"],
            required: []
        },
        intermediate: {
            name: "The Tree 🌳🌳",
            price: 499,
            features: ["7 languages (including German, Spanish, English)", "2,000 minutes of calls/month", "Task automation + financial forecasting", "Advanced dashboard", "18 hours/day technical support"],
            suitableFor: ["small", "medium"],
            required: ["languages"]
        },
        advanced: {
            name: "The Forest 🌳🌳🌳",
            price: 999,
            features: ["15 languages with instant translation", "5,000 minutes of calls/month", "Custom AI", "CRM system integration", "24/7 technical support"],
            suitableFor: ["medium", "large"],
            required: ["languages", "ai"]
        },
        premium: {
            name: "The Crown 👑",
            price: 1999,
            features: ["25 languages with local dialects", "10,000 minutes of calls/month", "Ultra-fast query processing (&lt;5 seconds)", "Predictive analytics based on ML", "Advanced security package (GDPR, ISO 27001)"],
            suitableFor: ["large", "enterprise"],
            required: ["languages", "security"]
        },
        professional: {
            name: "The Scepter 🔱",
            price: 3999,
            features: ["40+ languages with cultural translation", "Unlimited minutes", "Client-owned AI (White-label)", "Global coverage with local compliance", "Dedicated support team under your brand"],
            suitableFor: ["enterprise"],
            required: []
        },
        exceptional: {
            name: "The Throne 🏰",
            price: "Contact us",
            features: ["Tailored solutions", "99.99% SLA", "Multi-region management with global infrastructure", "Strategic consultations from top experts", "3 free months with annual subscription"],
            suitableFor: ["enterprise"],
            required: ["customization", "security"]
        }
    };

    const state = {
        size: null,
        budget: null,
        features: []
    };

    let timeoutId;

    // Manage steps
    function showStep(step) {
        document.querySelectorAll('.step-card').forEach(el =&gt; el.classList.remove('active'));
        document.querySelector(`[data-step="${step}"]`).classList.add('active');
        document.querySelector('.progress-fill').style.width = `${(step/3)*100}%`;
    }

    // Company size selection
    document.querySelectorAll('.size-option').forEach(option =&gt; {
        option.addEventListener('click', function() {
            state.size = this.dataset.size;
            document.querySelectorAll('.size-option').forEach(el =&gt; el.classList.remove('selected'));
            this.classList.add('selected');
            showStep(2);
        });
    });

    // Budget input with delay
    document.getElementById('budgetInput').addEventListener('input', function(e) {
        clearTimeout(timeoutId);
        timeoutId = setTimeout(() =&gt; {
            const value = parseInt(e.target.value);
            if (value &gt; 0) {
                state.budget = value;
                showStep(3);
                calculatePlan();
            }
        }, 500);
    });

    // Feature selection
    document.querySelectorAll('input[name="features"]').forEach(checkbox =&gt; {
        checkbox.addEventListener('change', function() {
            state.features = Array.from(document.querySelectorAll('input[name="features"]:checked'))
                                .map(el =&gt; el.value);
            calculatePlan();
        });
    });

    // Calculate the best plan
    function calculatePlan() {
        if (!state.size || !state.budget) return;

        let bestPlan = plans.basic;
        let maxScore = 0;

        Object.values(plans).forEach(plan =&gt; {
            let score = 0;
            
            if (plan.suitableFor.includes(state.size)) score += 40;
            if (typeof plan.price === 'number' &amp;&amp; state.budget &gt;= plan.price) score += 30;
            score += plan.required.filter(req =&gt; state.features.includes(req)).length * 20;

            if (score &gt; maxScore) {
                maxScore = score;
                bestPlan = plan;
            }
        });

        // Display the best plan
        document.getElementById('recommendedPlan').textContent = bestPlan.name;
        document.getElementById('planPrice').textContent = typeof bestPlan.price === 'number' 
            ? `Price: $${bestPlan.price}/month` 
            : bestPlan.price;
        
        document.getElementById('planFeatures').innerHTML = `
            &lt;h3&gt;Features:&lt;/h3&gt;
            &lt;ul&gt;${bestPlan.features.map(f =&gt; `&lt;li&gt;${f}&lt;/li&gt;`).join('')}&lt;/ul&gt;
        `;
    }

    // Initial setup
    showStep(1);
});
</script>

</template>
