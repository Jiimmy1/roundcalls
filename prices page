        <template class="s_embed_code_saved"><meta charset="UTF-8"/>
<title>Smart Plan Selector</title>
<style>
    :root {
        --primary-color: #2E7D32;
        --secondary-color: #81C784;
        --accent-color: #FFD54F;
    }

    .oe_website_login_container {
        max-width: 1200px;
        margin: 2rem auto;
        padding: 20px;
        background: white;
        border-radius: 20px;
        box-shadow: 0 10px 30px rgba(0,0,0,0.1);
    }

    .progress-bar {
        height: 8px;
        background: #eee;
        margin-bottom: 20px;
    }

    .progress-fill {
        height: 100%;
        background: var(--primary-color);
        transition: width 0.3s ease;
    }

    .step-card {
        display: none;
        padding: 20px;
    }

    .step-card.active {
        display: block;
        animation: fadeIn 0.3s ease;
    }

    .company-size-selector {
        display: grid;
        grid-template-columns: repeat(auto-fit, minmax(180px, 1fr));
        gap: 1rem;
    }

    .size-option {
        border: 2px solid #ddd;
        border-radius: 12px;
        padding: 1rem;
        cursor: pointer;
        transition: all 0.2s ease;
    }

    .size-option.selected {
        border-color: var(--primary-color);
        background: #E8F5E9;
    }

    .budget-input {
        width: 100%;
        padding: 1rem;
        border: 2px solid var(--primary-color);
        border-radius: 8px;
        margin: 1rem 0;
        font-size: 1.1rem;
    }

    .feature-selector {
        display: grid;
        grid-template-columns: repeat(2, 1fr);
        gap: 1rem;
    }

    .feature-checkbox {
        padding: 1rem;
        background: #f8f9fa;
        border-radius: 8px;
    }

    .result-card {
        background: var(--primary-color);
        color: white;
        border-radius: 12px;
        padding: 2rem;
    }

    .plan-card {
        background: #f5f5f5;
        border-radius: 15px;
        padding: 1.5rem;
        margin-top: 1.5rem;
        box-shadow: 0 10px 30px rgba(0, 0, 0, 0.1);
        text-align: center;
    }

    .plan-card h3 {
        margin: 0.5rem 0;
        font-size: 1.8rem;
    }

    .plan-card .price {
        font-size: 1.6rem;
        margin: 1rem 0;
        font-weight: bold;
    }

    .plan-card .features {
        margin: 1rem 0;
        font-size: 1.1rem;
        text-align: left;
    }

    .plan-card .features li {
        margin-bottom: 0.5rem;
    }

    .plan-card .cta {
        margin-top: 1rem;
        font-size: 1.2rem;
        color: var(--primary-color);
        font-weight: bold;
    }

    @keyframes fadeIn {
        from { opacity: 0; transform: translateY(10px); }
        to { opacity: 1; transform: translateY(0); }
    }
</style>

<div class="oe_website_login_container">
    <div class="progress-bar">
        <div class="progress-fill" style="width: 33%"/>
    </div>

    <div class="step-card active" data-step="1">
        <h2>🏢 Choose Your Company Size</h2>
        <div class="company-size-selector">
            <div class="size-option" data-size="individual">
                <h3>Individual/Startup</h3>
                <p>(1-5 employees)</p>
            </div>
            <div class="size-option" data-size="small">
                <h3>Small</h3>
                <p>(6-20 employees)</p>
            </div>
            <div class="size-option" data-size="medium">
                <h3>Medium</h3>
                <p>(21-100 employees)</p>
            </div>
            <div class="size-option" data-size="large">
                <h3>Large</h3>
                <p>(101-500 employees)</p>
            </div>
            <div class="size-option" data-size="enterprise">
                <h3>Enterprise</h3>
                <p>(500+ employees)</p>
            </div>
        </div>
    </div>

    <div class="step-card" data-step="2">
        <h2>💰 Enter Your Monthly Budget ($)</h2>
        <input type="number" class="budget-input" id="budgetInput" placeholder="e.g., 500"/>

        <h2 style="margin-top: 1.5rem">🎯 Select Required Features</h2>
        <div class="feature-selector">
            <label class="feature-checkbox">
                <input type="checkbox" name="features" value="languages"/> 
                Multi-language Support
            </label>
            <label class="feature-checkbox">
                <input type="checkbox" name="features" value="ai"/> 
                Artificial Intelligence
            </label>
            <label class="feature-checkbox">
                <input type="checkbox" name="features" value="support"/> 
                24/7 Support
            </label>
